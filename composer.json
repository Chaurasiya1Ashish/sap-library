{
  "name": "vikasmahato/sap-library",
  "description": "A PHP library for SAP integration",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vikas Mahato",
      "email": "vikas.prasad.mahato@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "SAPLibrary\\": "src/"
    },
    "classmap": [
    ],
    "files": [
      "src/SAPLibraryServiceProvider.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SAPLibrary\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/vikasmahato/sap-library.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  }
}
